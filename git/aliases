alias gp='git pull'
alias gpgp='git pull && git push'

alias gc='git commit'
alias gca='git commit -a'
alias gcm='git commit -m'

alias gco='git checkout'

alias gs='git status' 

alias gb='git branch'

alias gl='git log'

alias ga='git add'
alias gap='git add -p'
alias gaa='git add -A'

# Undo the last commit(unpushed)
alias peel='git reset HEAD^'

# Stash unstaged changes
alias clean='git stash --keep-index'
